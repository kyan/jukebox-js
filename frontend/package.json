{
  "homepage": "https://kyan.github.io",
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "classnames": "^2.2.5",
    "media-progress-timer": "^3.0.0",
    "react": "^16.2.0",
    "react-dnd": "^2.5.4",
    "react-dnd-html5-backend": "^2.5.4",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "react-scripts": "1.0.15",
    "redux": "^3.7.2",
    "reselect": "^3.0.1",
    "semantic-ui-react": "^0.75.0"
  },
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "standard && react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "enzyme-to-json": "^3.2.2",
    "gh-pages": "^1.1.0",
    "mock-socket": "^7.1.0",
    "react-dnd-test-backend": "^2.5.4",
    "react-test-renderer": "^16.1.1",
    "redux-devtools": "^3.4.0",
    "redux-mock-store": "^1.3.0",
    "shipit-cli": "^3.0.0",
    "standard": "^11.0.0-beta.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!<rootDir>/build/",
      "!src/__mockData__/",
      "!src/index.js",
      "!src/**/url-drop-area/index.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "standard": {
    "parser": "babel-eslint",
    "globals": [
      "describe",
      "context",
      "before",
      "beforeEach",
      "after",
      "afterEach",
      "it",
      "expect",
      "jest",
      "spyOn",
      "jasmine",
      "WebSocket"
    ]
  }
}
