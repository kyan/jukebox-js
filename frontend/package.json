{
  "homepage": "https://kyan.github.io",
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "deploy": "gh-pages -d build",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "standard && react-scripts test --env=jsdom",
    "coverage": "standard && react-scripts test --env=jsdom --coverage",
    "fix": "standard --fix",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "create-react-app": "^3.4.1",
    "dateformat": "^3.0.3",
    "media-progress-timer": "^3.0.0",
    "rc-progress": "^2.3.0",
    "rc-slider": "^9.2.1",
    "react": "^16.8.6",
    "react-audio-player": "^0.11.0",
    "react-dnd": "^7.4.5",
    "react-dnd-html5-backend": "^7.4.4",
    "react-dom": "^16.8.6",
    "react-notifications-component": "^2.3.0",
    "react-redux": "^7.1.0",
    "react-scripts": "^3.4.1",
    "react-use-googlelogin": "^0.11.1",
    "redux": "^4.0.4",
    "redux-devtools-extension": "^2.13.8",
    "reselect": "^4.0.0",
    "semantic-ui-react": "^0.88.1",
    "socket.io-client": "^2.2.0",
    "worker-timers": "^5.0.35"
  },
  "devDependencies": {
    "@types/jest": "^26.0.7",
    "@types/node": "^14.0.26",
    "@types/react": "^16.9.43",
    "@types/react-dom": "^16.9.8",
    "@types/react-notifications-component": "^2.4.0",
    "@types/react-redux": "^7.1.9",
    "@types/socket.io-client": "^1.4.33",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.3.5",
    "react-dnd-test-backend": "^7.4.4",
    "react-test-renderer": "^16.8.6",
    "redux-devtools": "^3.5.0",
    "redux-mock-store": "^1.5.3",
    "socket.io-mock": "^1.2.3",
    "standard": "^12.0.1",
    "typescript": "^3.9.7"
  },
  "jest": {
    "collectCoverageFrom": [
      "!<rootDir>/build/**",
      "!src/index.tsx",
      "!src/react-app-env.d.ts",
      "!src/constants/*.js",
      "!src/**/constants.js",
      "!src/components/url-drop-area/*.js",
      "!src/components/error-boundary/*.js",
      "src/**/*.{ts,tsx,js,jsx}"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  },
  "standard": {
    "parser": "babel-eslint",
    "globals": [
      "describe",
      "context",
      "before",
      "beforeEach",
      "after",
      "afterEach",
      "it",
      "expect",
      "jest",
      "spyOn",
      "jasmine",
      "WebSocket"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
