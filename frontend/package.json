{
  "homepage": "https://kyan.github.io",
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "deploy": "gh-pages -d build",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "standard && react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "create-react-app": "^3.0.1",
    "dateformat": "^3.0.3",
    "media-progress-timer": "^3.0.0",
    "rc-progress": "^2.3.0",
    "react": "^16.8.6",
    "react-audio-player": "^0.11.0",
    "react-dnd": "^7.4.5",
    "react-dnd-html5-backend": "^7.4.4",
    "react-dom": "^16.8.6",
    "react-notify-toast": "^0.5.0",
    "react-redux": "^6.0.1",
    "react-scripts": "^3.0.1",
    "redux": "^4.0.1",
    "redux-persist": "^5.10.0",
    "reselect": "^4.0.0",
    "semantic-ui-react": "^0.84.0",
    "socket.io-client": "^2.2.0"
  },
  "devDependencies": {
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.13.1",
    "enzyme-to-json": "^3.3.5",
    "socket.io-mock": "^1.2.3",
    "react-dnd-test-backend": "^7.4.4",
    "react-test-renderer": "^16.8.6",
    "redux-devtools": "^3.5.0",
    "redux-mock-store": "^1.5.3",
    "standard": "^12.0.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!<rootDir>/build/**",
      "!src/index.js",
      "!src/constants/*.js",
      "!src/components/url-drop-area/*.js",
      "!src/components/error-boundary/*.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "standard": {
    "parser": "babel-eslint",
    "globals": [
      "describe",
      "context",
      "before",
      "beforeEach",
      "after",
      "afterEach",
      "it",
      "expect",
      "jest",
      "spyOn",
      "jasmine",
      "WebSocket"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
